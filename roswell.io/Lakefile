(in-package :cl-user)
(defpackage :lake.user
  (:use :cl :lake :cl-syntax)
  (:shadowing-import-from :lake
                          :directory))
(in-package :lake.user)

(use-syntax :interpol)

(task "default" ()
  (mapc #'sh
        `(,(format nil "git clone -b ci https://~A:x-oauth-basic@github.com/roswell/roswell.github.io.git" (ros:getenv "GITHUB_OAUTH_TOKEN"))
          "cd roswell.github.io;make"
          )))



